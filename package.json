{
  "name": "template-typscript",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/jeflijonathan/typescript-template#readme",
  "bugs": {
    "url": "https://github.com/jeflijonathan/typescript-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeflijonathan/typescript-template.git"
  },
  "license": "ISC",
  "author": "jeflijonathan",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node -r dotenv/config -r tsconfig-paths/register dist/index.js",
    "check-env": "node -r dotenv/config -e \"console.log(\"DATABASE_URL=\", process.env.DATABASE_URL)\"",
    "preseed": "node -e \"const fs=require('fs'); if(!fs.existsSync('.env')) fs.copyFileSync('example.env','.env');\"",
    "seed": "npx ts-node -r tsconfig-paths/register -r dotenv/config src/seeder/index.ts",
    "postinstall": "npx prisma generate",
    "migrate": "npx prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:down": "npx prisma migrate down",
    "migrate:up": "npx prisma migrate up",
    "migrate:diff": "npx prisma migrate diff",
    "migrate:resolve": "npx prisma migrate resolve",
    "migrate:resolve:forward": "npx prisma migrate resolve --applied",
    "migrate:resolve:backward": "npx prisma migrate resolve --rolled-back",
    "migrate:resolve:latest": "npx prisma migrate resolve --rolled-back",
    "migrate:resolve:latest:forward": "npx prisma migrate resolve --applied",
    "migrate:resolve:latest:backward": "npx prisma migrate resolve --rolled-back",
    "migrate:resolve:latest:forward:backward": "npx prisma migrate resolve --rolled-back",
    "backend:dev": "npm run dev --prefix",
    "frontend:dev": "npm run dev --prefix"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@headlessui/react": "^2.2.7",
    "@mui/x-charts": "^8.22.1",
    "@prisma/adapter-mariadb": "^7.1.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@types/request-ip": "^0.0.41",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mysql2": "^3.15.3",
    "nodemailer": "^7.0.5",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "request-ip": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.0.4",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.2",
    "@types/nodemailer": "^6.4.17",
    "@types/pdfkit": "^0.17.2",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.4",
    "jest": "^30.0.4",
    "prisma": "^7.1.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "directories": {
    "doc": "doc"
  },
  "keywords": []
}
